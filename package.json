{
  "name": "@thebespokepixel/xo-tidy",
  "version": "0.2.2",
  "description": "post-babel xo formatting.",
  "main": "index.js",
  "bin": {
    "xo-tidy": "./bin/xoTidy.js"
  },
  "scripts": {
    "test": "gulp test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MarkGriffiths/xo-tidy.git"
  },
  "keywords": [
    "xo",
    "js",
    "javascript",
    "esformatter",
    "style",
    "standard"
  ],
  "author": "Mark Griffiths <mark@thebespokepixel.com> (http://thebespokepixel.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MarkGriffiths/xo-tidy/issues"
  },
  "homepage": "https://github.com/MarkGriffiths/xo-tidy#readme",
  "dependencies": {
    "babel-eslint": "^5.0.0-beta9",
    "cardinal": "^0.6.0",
    "deep-assign": "^2.0.0",
    "esformatter": "^0.9.0",
    "esformatter-dot-notation": "^1.3.1",
    "esformatter-limit-linebreaks": "0.0.3",
    "esformatter-parseint": "^1.0.3",
    "esformatter-semicolons": "^1.1.2",
    "esformatter-var-each": "^2.1.0",
    "eslint": "^2.0.0",
    "eslint-config-xo": "^0.9.2",
    "eslint-plugin-babel": "^3.1.0",
    "get-stdin": "^5.0.1",
    "gulp-util": "^3.0.7",
    "pkg-conf": "^1.1.1",
    "rocambole-token": "^1.2.1",
    "through2": "^2.0.1",
    "trucolor": "*",
    "truwrap": "*",
    "update-notifier": "^0.6.0",
    "verbosity": "*",
    "yargs": "^3.32.0"
  },
  "devDependencies": {
    "@thebespokepixel/cordial": "*",
    "ava": "^0.11.0",
    "semver-regex": "^1.0.0",
    "xo": "^0.12.1"
  },
  "directories": {
    "test": "test",
    "lib": "lib"
  },
  "preferGlobal": true,
  "xo": {
    "semicolon": false,
    "esnext": true,
    "rules": {
      "prefer-const": 1,
      "no-var": 0,
      "camelcase": [
        1,
        {
          "properties": "always"
        }
      ]
    }
  },
  "build_number": 77
}
